   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "\n",
    "def gauss(size,sigma):\n",
    "    r = range(-int(size/2),int(size/2)+1)\n",
    "    return [1 / (sigma * sqrt(2*pi)) * exp(-float(x)**2/(2*sigma**2)) for x in r]"
   ]
  },
